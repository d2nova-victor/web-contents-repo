<script type="text/javascript">
	$(document).ready(function(){
		var divHeight = $('.customer-feedback-img').height(); 
		$('.setH').css('height', divHeight+'px');
		if($(window).width() <= 840 ){
			$('.setH').css('height','100%');
		}
	});
	$( window ).on("resize", function() {
       var divHeight = $('.customer-feedback-img').height(); 
		$('.setH').css('height', divHeight+'px');
		if($(window).width() <= 840 ){
			$('.setH').css('height','100%');
		}

    });

</script>
