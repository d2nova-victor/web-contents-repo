    </script>
    <style>		
  p, a, h4, span, div h1, h2, h3 {font-family:"SF UI Text", sans-serif,source-han-sans-traditional, "Han Sans", "Noto Sans", "Noto Sans CJK TC",sans-serif !important;}</style>
  <link href="https://cdn2.hubspot.net/hubfs/1864133/EVOX_website_CSS/new-web-style/face-one.css" rel="stylesheet">
  <link href="https://cdn2.hubspot.net/hubfs/1864133/EVOX_website_CSS/new-web-style/share.css" rel="stylesheet">
    <link href="https://cdn2.hubspot.net/hubfs/1864133/EVOX_website_CSS/new-web-style/phase-one-for-EN.css" rel="stylesheet">
    <!-- font awesome icon -->
  <link href="https://cdn2.hubspot.net/hubfs/1864133/EVOX_Fonts/fontawesome/css/all.css" rel="stylesheet">
  <script src="https://kit.fontawesome.com/292c84d8b9.js"></script>
