          <script>
            $(function() {
              var w = window.innerWidth;
              var x = document.getElementById("myLinks");
              $('.mobile-menu-icon').click(function(e){
                this.classList.toggle("change");
                $("#myLinks").slideToggle()
              })
            });
             $(document).ready(function(){


              var topmenu = $(".menu-item");
              var listmenu = $(".drop-down-block");

               if($(window).width() <= 952){
                $(".drop-down-block").css("display","none");
              $("#headerNav-dropdown > a").each(function (i){
                $("#headerNav-dropdown > a").click(function(){
                  if($(listmenu[i]).css("display")=="block"){
                    $(listmenu[i]).slideUp(300);
                    $(topmenu[i]).css("display","none");
                    // $(listmenu[i]).css("display","none");
                  }else{
                    for(var j = 0; j < listmenu.length; j++){
                      $(listmenu[i]).slideUp(300);
                      $(topmenu[i]).css("display","none");
                      // $(listmenu[i]).css("display","none");
                    }
                    // if($(listmenu[i]).css("display")=="none"){
                    //   $(listmenu[i]).slideDown(300);
                    //   $(listmenu[i]).css("display","block")
                    //   $(topmenu[i]).css("display","block")
                    // }
                    $(listmenu[i]).slideDown(300);
                    $(listmenu[i]).css("display","block")
                    $(topmenu[i]).css("display","block")
                  }
                });
              });

            };
            if($(window).width() >= 952 ){
              $(".drop-down-block").css("display","block");
            };
              
            }); 

            </script>
