            .bar1, .bar2, .bar3 {
              width: 16px;
              height: 2px;
              background-color: #333;
              margin: 3px 0;
              transition: 0.4s;
              border-radius: 100px;
            }

            .change .bar1 {
              -webkit-transform: rotate(-45deg) translate(-1px, 3px);
                  transform: rotate(-45deg) translate(-1px, 3px);
            }

            .change .bar2 {opacity: 0;}

            .change .bar3 {
